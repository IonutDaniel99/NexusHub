// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id           Int     @id @default(autoincrement())
  uuid_string  String
  account_name String  @unique
  latitude     Float?
  longitude    Float?
  city_name    String?
}
